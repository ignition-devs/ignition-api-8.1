class SProcCall(Object):
    class SProcArg(Object):
        outParam: bool
        inParam: bool
        paramType: int
        value: Object
        def getParamType(self) -> int: ...
        def getValue(self) -> Any: ...
        def isInParam(self) -> bool: ...
        def isOutParam(self) -> bool: ...
        def setParamType(self, paramType: int) -> None: ...
        def setValue(self, value: Object) -> None: ...

    class SProcArgKey(Object):
        index: int
        name: Union[str, unicode]
        def getParamIndex(self) -> int: ...
        def getParamName(self) -> Union[str, unicode]: ...
        def isNamedParam(self) -> bool: ...

    callFinished: bool
    datasource: Union[str, unicode]
    params: Dict[SProcCall.SProcArgKey, SProcCall.SProcArg]
    procedureName: Union[str, unicode]
    resultset: Dataset
    returnParam: SProcCall.SProcArg
    skipAudit: bool
    txId: Union[str, unicode]
    updateCount: int
    def getDataSource(self) -> Union[str, unicode]: ...
    def getOutParamValue(self, param: Union[int, str, unicode]) -> Any: ...
    def getProcedureName(self) -> Union[str, unicode]: ...
    def getResultSet(self) -> BasicDataset: ...
    def getReturnValue(self) -> Any: ...
    def getTxId(self) -> Union[str, unicode]: ...
    def getUpdateCount(self) -> int: ...
    def isSkipAudit(self) -> bool: ...
    def registerInParam(
        self, param: Union[int, str, unicode], typeCode: int, value: Any
    ) -> None: ...
    def registerOutParam(
        self, param: Union[int, str, unicode], typeCode: int
    ) -> None: ...
    def registerReturnParam(self, typeCode: int) -> None: ...
    def setSkipAudit(self, skipAudit: bool) -> None: ...
    def setTxId(self, txId: Union[str, unicode]) -> None: ...

