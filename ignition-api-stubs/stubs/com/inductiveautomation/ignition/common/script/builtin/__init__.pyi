class AbstractOPCUtilities(Object):
    class PyOPCTag(PyObject):
        displayName: Union[str, unicode]
        elementType: BrowseElementType
        nodeId: Union[str, unicode]
        serverName: Union[str, unicode]
        def __init__(
            self,
            serverName: Union[str, unicode],
            nodeId: Union[str, unicode],
            displayName: Union[str, unicode],
            elementType: BrowseElementType,
        ) -> None: ...
        def __findattr_ex__(self, name: Union[str, unicode]) -> PyObject: ...
        def getDisplayName(self) -> Union[str, unicode]: ...
        def getElementType(self) -> BrowseElementType: ...
        def getNodeId(self) -> Union[str, unicode]: ...
        def getServerName(self) -> Union[str, unicode]: ...

    def browseServer(
        self, opcServer: Union[str, unicode], nodeId: Union[str, unicode]
    ) -> List[AbstractOPCUtilities.PyOPCTag]: ...
    def getServers(
        self, *args: PyObject, **kwargs: Union[str, unicode]
    ) -> List[Union[str, unicode]]: ...
    def getServerState(self, opcServer: Union[str, unicode]) -> Union[str, unicode]: ...
    def isServerEnabled(self, serverName: Union[str, unicode]) -> bool: ...
    def readValue(
        self, opcServer: Union[str, unicode], itemPath: Union[str, unicode]
    ) -> QualifiedValue: ...
    def readValues(
        self, opcServer: Union[str, unicode], itemPaths: List[Union[str, unicode]]
    ) -> QualifiedValue: ...
    def setServerEnabled(
        self, serverName: Union[str, unicode], enabled: bool
    ) -> None: ...
    def writeValue(
        self, *args: PyObject, **kwargs: Union[str, unicode]
    ) -> QualityCode: ...
    def writeValues(
        self, *args: PyObject, **kwargs: Union[str, unicode]
    ) -> List[QualityCode]: ...

