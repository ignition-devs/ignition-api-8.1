    def getCurrentWindow(self) -> Union[str, unicode]: ...
    def goBack(self) -> PyObject: ...
    def goForward(self) -> PyObject: ...
    def goHome(self) -> PyObject: ...
    def openWindow(
        self,
        path: Union[str, unicode],
        params: Optional[Dict[Union[str, unicode], Any]] = ...,
    ) -> PyObject: ...
    def openWindowImpl(
        self,
        path: Union[str, unicode],
        params: Dict[Union[str, unicode], Any],
        openAdditional: bool,
    ) -> PyObject: ...
    def openWindowInstance(
        self,
        path: Union[str, unicode],
        params: Optional[Dict[Union[str, unicode], Any]] = ...,
    ) -> PyObject: ...
    def swapTo(
        self, name: Union[str, unicode], params: Dict[Union[str, unicode], Any]
    ) -> PyObject: ...
    def swapWindow(self, *args: Any) -> PyObject: ...

